                        'theme' : 'light'
                    });
                });
            }

            function animateCoin(start, end)
            {
                var img = $('<img src="http://www.dlcompare.fr/bundles/dlcomparemain/img/dlcoins.gif" />');
                img.css('position', 'absolute');
                img.css('width', 25);
                img.css('height', 25);
                img.css('display', 'none');
                img.offset(start);

                $('body').append(img);
                img.fadeIn().animate({top: end.top+"px", left:end.left+"px"}, 500, "easeOutBounce", function() { $(this).fadeOut('fast').remove(); });  

                var audio = new Audio('/bundles/dlcomparemain/img/coin_flip.wav');
                audio.play();
            }

            function launchCoinAnimation(target, points)
            {
                end = $('.login-status img').offset();
                start = $(target).offset();

                var doAnimateCoin = function() { animateCoin(start, end); }

                doAnimateCoin();
                setTimeout(doAnimateCoin, 100);
                setTimeout(doAnimateCoin, 200);
                setTimeout(doAnimateCoin, 300);
                setTimeout(doAnimateCoin, 400);

                var $this = $('.login-status .points');
                $({ counter: $this.text() }).animate({ counter: points }, {
                    duration: 1000,
                    easing: 'swing',
                    step: function (i) 
                    {
                        $this.text(Math.ceil(i));
                    }
                });
            }

            var img = $('<img src="http://www.dlcompare.fr/bundles/dlcomparemain/img/dlcoins.gif" />');
            img.css('display', 'none');
            $('body').append(img);
            img.remove();
        </script>
